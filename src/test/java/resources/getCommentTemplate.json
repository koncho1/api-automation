{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/entry-schema",
  "type": "object",
  "properties": {
    "id": {
      "description": "The unique identifier for a product",
      "type": "integer"
    },
    "post_id": {
      "type": "integer"
    },
    "name": {
      "description": "Name of the user",
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "body": {
      "type": "string"
    }
  },
  "required": ["id", "post_id", "name", "email","body"]
}